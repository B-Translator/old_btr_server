server {
    listen 0.0.0.0:8080;
    root /var/www/;
    index index.html index.php;

    include /etc/nginx/include/php;
}

server {
    listen 0.0.0.0:8888;
    root /var/www/;
    index index.html index.php;

    fastcgi_param HTTPS on;
    include /etc/nginx/include/ssl;
    include /etc/nginx/include/php;
}


